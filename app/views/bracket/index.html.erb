<h1>Creat a new bracket</h1>
<div ng-controller="BracketController">
  <form ng-submit="addEntry(newEntry)">
  <input type="text" ng-model="newEntry" />
  {{newEntry}}
  <input type="submit" value="Add" />
  </form>
  <ul>
    <li ng-repeat="entry in entries">
      <p id="{{entry.id}}">{{entry.name}}</p>
    </li>
  </ul>
</div>